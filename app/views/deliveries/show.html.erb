<section class="content-header">
	<h1>
		Visualizar Entrega de Software
	</h1>
	<ol class="breadcrumb">
		<li><a href="/"><i class="fa fa-dashboard"></i> DashBoard</a></li>
		<li class="active">Entregas de Software</li>
	</ol>
</section>

<section class="content">
  <div class="row">
    <div class="col-md-12">
      <div class="box box-info">

        <div class="col-xs-12">
          <h2 class="page-header">
            <i class="fa fa-globe"></i>  <%= @delivery.project_id %>
            <small class="pull-right">Data Cadastro: <%= @delivery.created_at.to_s(:long) %></small>
          </h2>
        </div>



        <div class="box-body">

          <div class="row invoice-info">
            <div class="col-sm-6 invoice-col">
              <address>
                <strong>Dados da Entrega de Software</strong><br>
                Projeto: <%= @delivery.project_id %><br>
                Descrição: <%= @delivery.software %><br>
                Versão: <%= @delivery.versao %>
              </address>
            </div>
            <!-- /.col -->
            <div class="col-sm-6 invoice-col">
              <address>
                <strong>Dados de Contato</strong><br>
              </address>
            </div>
          </div>


          <div class="row no-print">
            <div class="col-xs-12">

              <%= link_to '<i class="fa fa-arrow-left"> Voltar</i>'.html_safe, :deliveries, :class => 'btn btn-success' %>

              <%= link_to '<i class="fa fa-pencil"> Editar</i>'.html_safe, edit_delivery_path, :class => 'btn btn-primary pull-right' %>

<br>
<br>
              <%= form_tag @delivery, :method => 'delete' do %>

                <%= button_tag class: "btn btn-primary pull-right" do %>
                <i class="fa fa-times"> Excluir</i>
                <% end %>

              <% end %>

            </div>
          </div>



        </div>


      </div>
    </div>
  </div>
</section>
</section>